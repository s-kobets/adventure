.countries__content__tooltip,
.countries__content__tooltip-filter,
.countries__content__tooltip-content
	display none
	position absolute
	z-index 10
	top 120px
	left 0
	padding 10px
	width 250px	
	background #FFF 
	opacity .85 
	border 2px solid #ccc
	border-radius 5px
	font-size 18px
	color #2424C2
	text-align left

	& span
		color red	
		
	&:before
		content ''	
		position absolute
		top -18px					
		display block
		border-bottom 18px solid #ccc
		border-left 18px solid transparent
		border-right 18px solid transparent

.countries__content__tooltip-filter
	width 346px
	top -58px

	&:before
		top 38px
		border-top 18px solid #ccc
		border-bottom 0

.countries__content__tooltip-content
	width 500px	
	top -58px
	left 450px

	&:before
		top 74px
		border-top 18px solid #ccc
		border-bottom 0

.countries__content__blocks	
	margin-top 20px	
	text-align center


	&__item
		display inline-block
		float left
		width 620px		
		
		@media (max-width: 639px)
			@media (min-width: 320px)
				width 300px
				float none
		
		@media (max-width: 1279px)
			@media (min-width: 640px)			
				float none		

		&-one
			float left
			width 300px			
						
		&__cuba,
		&__cuba_disable
			position relative
			display inline-block
			float left
			width 300px
			height 200px
			margin-left 0
			
			&__one
				//display none
				width 100%
				height 200px
				background url('../images/cuba.jpg') no-repeat center center
				background-size cover
								
				&__overlay
					position absolute
					top 0
					left 0
					width 100%
					height 100%
					z-index 2
					background rgba(3,21,36,.6)
					
					& h1
						display block
						margin-top 91.5px
						font 36px/17px 'NewBaskervilleC', sans-serif
						color #fff 
						text-align center
			
			&__two
				//display none
				position absolute
				top 0
				left 0
				width 100%
				height 100%
				background #313336
				
				&__text
					padding-top 40px
					padding-left 20px
					
					& h3
						width 260px
						text-align left
						font 14px/22px 'PTSansRegular', sans-serif
						color #abaeb3
				
					& h2
						margin-top 36px
						text-align left
						font 24px/17px 'PTSansRegular', sans-serif
						color #fff
					
			&__three
				//display none
				position absolute
				top 0
				left 0
				width 100%
				height 100%	
				background url('../images/cuba_three.jpg') no-repeat center center
				background-size cover	
				
				&__overlay
					position absolute
					top 0
					left 0
					width 100%
					height 100%
					z-index 2
					background rgba(3,21,42,.2) 				
					
					&:hover	
						background rgba(3,21,42,.2) url('../images/icon_zoom.svg') no-repeat center center	
						background-size 42px 42px
						cursor pointer
						
					& h2
						margin-left 20px
						margin-top 164px
						text-align left
						font 24px/17px 'PTSansRegular', sans-serif
						color #fff
				
			&__four	
				//display none
				position absolute
				top 0
				left 0
				width 100%
				height 100%		
				background url('../images/cuba_four.jpg') no-repeat center 28%
				background-size cover		
						
				&__overlay
					position absolute
					top 0
					left 0
					width 100%
					height 100%
					z-index 2
					background rgba(7,26,42,.2)	
					
					&:hover	
						background rgba(7,26,42,.2) url('../images/icon_play.svg') no-repeat center center	
						background-size 42px 42px
						cursor pointer				
					
					& h2
						margin-left 20px
						margin-top 164px
						text-align left
						font 24px/17px 'PTSansRegular', sans-serif
						color #fff

			&__first
				position absolute
				bottom 19px
				right 20px
				height 21px
				z-index 3				
				
				& #prewbutton
					float left
					width 21px
					height 21px
					opacity .15
					margin-right 16px					
					background url('../images/prev_slide.svg') no-repeat
					cursor pointer
					
					&:hover
						opacity .3
					
				& #nextbutton
					float right
					width 21px
					height 21px
					opacity .15									
					background url('../images/next_slide.svg') no-repeat
					cursor pointer
					
					&:hover
						opacity .3		
					
	&__item__cuba_disable
		display none			
			
		&__one			
			margin-top 20px
			margin-left 30px
			width 240px
			height 160px
			opacity .2
			
			&__overlay
				top 20px
				left 30px
				width 240px
				height 160px
				
				& h1
					margin-top 71.5px	
//Модальное окно image
.popup_overlay
	display none
	position fixed
	left 0
	top 0
	width 100%
	height 100%
	background: rgba(20,26,31,.9)
	text-align: center	
	z-index 10
	
	@media (max-width: 509px)
		display none
		background: transparent
		
	&:after
		content ''
		display inline-block
		height 100%
		width 0
		vertical-align middle		
  						
	& .popup
		position relative
		display inline-block			
		width 640px
		height 480px		
		background url('../images/cuba_three.jpg') no-repeat center center
		background-size cover
		vertical-align: middle
		z-index 11		

		@media (max-width: 710px)
			@media (min-width: 510px)
				width 440px
				height 248px
				
		@media (max-width: 509px)
			display none

		& a
			position absolute
			display block
			top -35px
			right -35px
			width 30px
			height 30px
			background url('../images/icon_close.svg') no-repeat center center
			opacity .2
			z-index 11
			
			&:hover
				opacity .5

//Модальное окно video
.video_overlay
	display none
	position fixed
	left 0
	top 0
	width 100%
	height 100%
	background: rgba(20,26,31,.9)
	text-align: center	
	z-index 10	
	
	@media (max-width: 799px)
		display none
		background: transparent
		
	&:after
		content ''
		display inline-block
		height 100%
		width 0
		vertical-align middle		
  						
	& .popup
		position relative
		display inline-block			
		width 740px
		height 480px			
		background-size cover
		vertical-align: middle
		z-index 11	
		
		@media (max-width: 799px)			
			display none
		
		& a
			position absolute
			display block
			top -35px
			right -35px
			width 30px
			height 30px
			background url('../images/icon_close.svg') no-repeat center center
			opacity .2
			z-index 11
			
			&:hover
				opacity .5			
						
@media only screen and (-webkit-min-device-pixel-ratio: 2), 
only screen and (min-moz-device-pixel-ratio: 2), 
only screen and (-o-min-device-pixel-ratio: 2/1), 
only screen and (min-device-pixel-ratio: 2), 
only screen and (min-resolution: 192dpi), 
only screen and (min-resolution: 2dppx)
	.countries__content__blocks__item__cuba__one
		background url('../images/cuba@2x.jpg') no-repeat center center
	.countries__content__blocks__item__cuba__three	
		background url('../images/cuba_three@2x.jpg') no-repeat center center
	.countries__content__blocks__item__cuba__four	
		background url('../images/cuba_four@2x.jpg') no-repeat center 28%
		
		